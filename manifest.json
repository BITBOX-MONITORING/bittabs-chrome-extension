{
  "name": "Bit Tabs",
  "manifest_version": 3,
  "version": "1.0",
  "description": "Extens√£o para monitorar abas em inatividade no navegador",
  "icons": {
    "16": "./public/assets/icon16.png",
    "32": "./public/assets/icon32.png",
